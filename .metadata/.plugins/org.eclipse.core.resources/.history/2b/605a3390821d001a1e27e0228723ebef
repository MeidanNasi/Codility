package lesson_4_couting_elements;

import java.util.HashMap;

public class PermCheck {
	    public int solution(int[] A) {
	        
	    	HashMap<Integer, Integer> mapper = new HashMap<>();
	    	
	    	for(int a : A) {
	    		if(mapper.containsKey(a)) {
	    			mapper.put(a, mapper.get(a)+1);
	    		}
	    		else mapper.put(a, 1);
	    	}
	    	
	    	
	    	
	    	return 0;
	    }
}
