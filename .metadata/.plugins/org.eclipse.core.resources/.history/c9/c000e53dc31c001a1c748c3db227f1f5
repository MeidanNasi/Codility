package lesson_2;

class Solution {
    public int solution(int[] A) {
    	int res = 0;
        for(int i =0; i<A.length; i++) {
        	for(int j=i+1; j<A.length; j++) {
        		if(A[i] == A[j]) break;
        		else if(j+1 > A.length) { res = i;}
        	}
        }
        return A[res];
    }
}