package lesson_15_catepillarMethod;

public class AbsDistinct {
	public int solution(int[] A) {

		int counter = 0;
		int len = A.length;
		boolean seen[] = new boolean[len];

		for (int i = 0; i < len; i++) {
			if (seen[Math.abs(A[i])] == false) {
				seen[Math.abs(A[i])] = true;
			} else
				counter++;
		}

		return len-counter;
	}
}
