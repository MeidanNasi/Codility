package lesson_2;

import java.util.HashMap;

class Solution {
    public int solution(int[] A) {
    	int res = 0;
    	boolean flag = false;
    	HashMap<Integer, Boolean> checker = new HashMap<>();
    	for(int i=0; i<A.length; i++) {
    		for(int j=i+1; j<A.length; j++) {
    			if(!checker.containsKey(A[i])) {
    				if(A[i] == A[j]) {
    					checker.put(A[i], true);
    					flag=true;
    					break;
    				}
    			}
    		}
    		if(flag == false) {
    			res = A[i];
    			break;
    		}
    	}
        return res;
    }
}