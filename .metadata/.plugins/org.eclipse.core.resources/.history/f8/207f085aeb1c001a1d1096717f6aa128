package lesson_3_time_comlexity;

public class TapeEquilibrium {
	public int solution(int[] A) {
		
		int subleft =0;
		int subright =0;
		int min = Integer.MAX_VALUE;
		int tmp =0;
		
		for(int i=0; i<A.length; i++) {
			subright = A[i];
		}
		
		for (int i = 0; i < A.length - 1; i++) {
			subleft += A[i];
			subright -= A[i];
			
			tmp = Math.abs(subleft-subright);
			if(tmp < min) min = tmp;
		}
		return min;
	}
}
