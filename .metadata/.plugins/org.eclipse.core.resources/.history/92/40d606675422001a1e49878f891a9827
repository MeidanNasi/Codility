
public class inserting {

	int solution(int N) {

		boolean flag = false;
		int len = 0;
		int newVal = 0;
		while (N / 10 != 0) {
			len++;
		}

		while (!flag) {
			if (N > 0) {
				if (N % 10 * len < 5) {
					newVal = 10 ^ len + N;
					flag = true;
				} else
					len--;
			}
		}

		return newVal;
	}
}
